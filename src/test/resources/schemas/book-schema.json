{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["id", "title", "author", "isbn", "publishedYear", "available"],
  "properties": {
    "id": {
      "type": "string"
    },
    "title": {
      "type": "string",
      "minLength": 1
    },
    "author": {
      "type": "string",
      "minLength": 1
    },
    "isbn": {
      "type": "string"
    },
    "publishedYear": {
      "type": "integer"
    },
    "available": {
      "type": "boolean"
    }
  }
}

